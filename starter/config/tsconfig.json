{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@starter": [
        "./starter/index.tsx"
      ],
      "@slice/*": [
        "./reducers/slice/*"
      ],
      "@slice": [
        "./reducers/slice/index.ts"
      ],
      "@screens/*": [
        "./screens/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@hooks": [
        "./hooks"
      ]
    }
  },
}