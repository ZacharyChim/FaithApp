{
  "name": "faith",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "start:pro": "ENVIRONMENT=production expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "build:ios": "eas build --profile development --platform ios --no-wait",
    "build:ios:pro": "eas build --profile production --platform ios --no-wait",
    "build:android": "eas build --profile development --platform android --no-wait",
    "build:android:pro": "eas build --profile production --platform android --no-wait",
    "publish": "eas update",
    "publish:web": "expo build:web && firebase deploy",
    "publish:pro": "ENVIRONMENT=production eas update",
    "publish:pro:web": "ENVIRONMENT=production expo build:web && firebase deploy"
  },
  "jest": {
    "preset": "jest-expo"
  },
